{
    "name": "who-jonson/laravel-organizer",
    "description": "A customizer for modifying Laravel structure",
    "keywords": ["php", "laravel", "custom laravel", "cli", "artisan", "command", "composer-package", "console"],
    "homepage": "https://github.com/who-jonson/laravel-organizer",
    "type": "package",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "composer/composer": "^1.6 || ^2",
        "illuminate/console": "^6 || ^7 || ^8",
        "illuminate/filesystem": "^6 || ^7 || ^8",
        "illuminate/support": "^6 || ^7 || ^8",
        "phpdocumentor/type-resolver": "^1.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^4 || ^5 || ^6",
        "phpunit/phpunit": "^8.5 || ^9"
    },
    "authors": [
        {
            "name": "Jonson B.",
            "email": "www.jbc.bd@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/who-jonson/laravel-organizer/issues",
        "source": "https://github.com/who-jonson/laravel-organizer"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-version": "1.0-dev",
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "WhoJonson\\LaravelOrganizer\\LaravelOrganizerServiceProvider",
                "WhoJonson\\LaravelOrganizer\\LaravelOrganizerMacroServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "WhoJonson\\LaravelOrganizer\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WhoJonson\\Tests\\LaravelOrganizer\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
